# MinerU-AI集成环境配置示例文件
# 复制此文件为 .env 并填入实际的配置值

# ==============================================
# DeepSeek API 配置
# ==============================================
# 获取API密钥: https://platform.deepseek.com/
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# DeepSeek API 基础URL (官方推荐)
DEEPSEEK_BASE_URL=https://api.deepseek.com

# 使用的模型 (V3.2-Exp版本)
DEEPSEEK_MODEL=deepseek-chat

# 思考模式模型 (V3.2-Exp思考模式)
DEEPSEEK_REASONER_MODEL=deepseek-reasoner

# ==============================================
# MinerU 服务配置
# ==============================================
# MinerU服务地址
MINERU_URL=http://localhost:8001

# MinerU服务超时时间 (秒)
MINERU_TIMEOUT=30

# ==============================================
# AI 服务配置
# ==============================================
# AI服务端口
AI_SERVICE_PORT=8206

# AI服务超时时间 (秒)
AI_SERVICE_TIMEOUT=30

# 最大重试次数
AI_SERVICE_MAX_RETRIES=3

# ==============================================
# 数据库配置
# ==============================================
# PostgreSQL 配置
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=szjason72
POSTGRES_PASSWORD=
POSTGRES_DB=jobfirst_vector

# MySQL 配置
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_USER=root
MYSQL_PASSWORD=
MYSQL_DB=dao_dev

# Redis 配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# ==============================================
# 日志配置
# ==============================================
# 日志级别: DEBUG, INFO, WARNING, ERROR
LOG_LEVEL=INFO

# 日志文件路径
LOG_FILE=logs/ai_service.log

# ==============================================
# 监控配置
# ==============================================
# 是否启用监控
ENABLE_MONITORING=true

# 监控端口
MONITORING_PORT=9090

# 健康检查间隔 (秒)
HEALTH_CHECK_INTERVAL=30

# ==============================================
# 开发配置
# ==============================================
# 开发模式
DEBUG=false

# 是否启用热重载
ENABLE_HOT_RELOAD=false

# 测试模式
TEST_MODE=false
